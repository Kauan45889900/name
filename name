

package llfd;
public class flassw {




	
	    private String nombre;
	    private double nota;

	    public flassw(String nombre, double nota) {
	        this.nombre = nombre;
	        this.nota = nota;
	    }

	    public String getNombre() {
	        return nombre;
	    }

	    public void setNombre(String nombre) {
	        this.nombre = nombre;
	    }

	    public double getNota() {
	        return nota;
	    }

	    public void setNota(double nota) {
	        this.nota = nota;
	    }

	    @Override
	    public String toString() {
	        return "Nombre: " + nombre + ", Nota: " + nota;
	    }

	    public static void main(String[] args) {
	        // Ejemplo de uso
	        flassw nota1 = new flassw("Juan", 8.5);
	        System.out.println(nota1);

	        // Modificar la nota
	        nota1.setNota(9.0);
	        System.out.println("Nueva nota de Juan: " + nota1.getNota());
	    }
	}
